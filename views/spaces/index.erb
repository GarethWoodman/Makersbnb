<% @spaces.each do |space| %>
  <%= space.name %> <br><br>
  <%= space.description %><br><br>
  <%= space.available_from.strftime("%d-%m-%Y") %><br><br>
  <%= space.available_to.strftime("%d-%m-%Y") %><br><br>
  <img src="<%= space.image_path %>" height="150" width="150"><br><br>
  <br><br><br><br>
<% end %>